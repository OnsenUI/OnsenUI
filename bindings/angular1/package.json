    "onsen",
    "onsenui"
  ],
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/OnsenUI/OnsenUI/issues"
  },
  "homepage": "https://onsen.io",
  "peerDependencies": {
    "onsenui": "~2.10.0"
  },
  "devDependencies": {
    "angular": "^1.7.2",
    "gulp-chmod": "^2.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-connect": "^5.5.0",
    "gulp-download": "^0.0.1",
