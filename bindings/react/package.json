{
  "name": "react-onsenui",
  "version": "1.2.0",
  "description": "Onsen UI - React Components for Hybrid Cordova/PhoneGap Apps with Material Design and iOS UI components",
  "scripts": {
    "build": "npm run lint && rollup -c",
    "prepublish": "npm run build",
    "postpublish": "npm run gen-docs",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --ext .js,.jsx src --fix",
    "dev": "webpack-dev-server --progress --host 0.0.0.0",
    "test": "karma start",
    "gen-docs": "node ./docgen.js"
  },
  "main": "dist/react-onsenui.js",
  "files": [
    "yarn.lock",
    "dist/react-onsenui.*",
    "src",
    "test",
    "scripts",
    "docs",
    ".eslintrc.json",
    "webpack.config.js",
    "rollup.config.js",
    "karma.conf.js",
    "tests.webpack.js",
    "docgen.js",
    "circle.yml",
    "index.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnsenUI/OnsenUI.git"
  },
  "keywords": [
    "react",
    "react-component",
    "phonegap",
    "cordova",
    "hybrid",
    "app",
    "mobile",
    "ui",
    "onsen",
    "onsenui"
  ],
  "author": "Onsen UI Team <team@monaca.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OnsenUI/OnsenUI/issues"
  },
  "homepage": "https://onsen.io/react/",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "css-loader": "0.26.0",
    "eslint": "^3.7.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "filter-files": "^0.4.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.1.2",
    "onsenui": "^2.0.3",
    "prompt": "^1.0.0",
    "react": "^15.4.2",
    "react-docgen": "^2.11.0",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^5.0.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "onsenui": "2.2.x",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
